{
  "name": "@overlays/react",
  "type": "module",
  "version": "0.2.0",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/unoverlays#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/unoverlays.git"
  },
  "bugs": {
    "url": "https://github.com/hairyf/unoverlays/issues"
  },
  "keywords": [
    "unified",
    "overlay",
    "react"
  ],
  "main": "./src/index.ts",
