{
  "name": "@unoverlays/react",
  "version": "0.3.1",
  "license": "MIT",
  "homepage": "https://github.com/TuiMao233/unoverlays#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TuiMao233/unoverlays.git"
  },
  "bugs": {
    "url": "https://github.com/TuiMao233/unoverlays/issues"
  },
  "keywords": [
    "unified",
    "overlay",
    "component",
    "popup",
    "react"
  ],
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "index.cjs.js",
    "types": "./index.d.ts",
    "module": "./index.esm.js",
    "unpkg": "./index.iife.min.js",
    "jsdelivr": "./index.iife.min.js",
    "exports": {
      ".": {
        "import": "./index.esm.js",
        "require": "./index.cjs.js"
      },
      "./*": "./*"
    },
    "directory": "dist"
  },
  "scripts": {
    "build": "ptsup -r src index.ts --dts --meta --clean --target es2020",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@unoverlays/utils": "workspace:*",
    "mitt": "^3.0.0",
    "pascal-case": "^3.1.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
