{
  "name": "unoverlay-vue",
  "version": "0.2.5",
  "main": "src/index.ts",
  "keywords": [
    "unified",
    "overlay",
    "vue overlay component",
    "overlay-vuejs",
    "overlay-vue",
    "vuejs-overlay",
    "vue-overlay",
    "popup",
    "vue",
    "component"
  ],
  "publishConfig": {
    "main": "index.cjs.js",
    "types": "./index.d.ts",
    "module": "./index.esm.js",
    "unpkg": "./index.iife.min.js",
    "jsdelivr": "./index.iife.min.js",
    "exports": {
      ".": {
        "import": "./index.esm.js",
        "require": "./index.cjs.js"
      },
      "./*": "./*"
    },
    "directory": "dist"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "build": "ptsup -r src index.ts --dts --meta --clean",
    "change": "bumpp"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TuiMao233/unoverlay-vue.git"
  },
  "bugs": {
    "url": "https://github.com/TuiMao233/unoverlay-vue/issues"
  },
  "homepage": "https://github.com/TuiMao233/unoverlay-vue#readme",
  "license": "MIT",
  "dependencies": {
    "@vueuse/core": "^8.7.4",
    "lodash": "^4",
    "mitt": "^3.0.0",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@types/lodash": "^4",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "jsdom": "^20.0.0",
    "ptsup": "^0.0.5",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitepress": "1.0.0-alpha.4",
    "vitest": "^0.15.2",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
